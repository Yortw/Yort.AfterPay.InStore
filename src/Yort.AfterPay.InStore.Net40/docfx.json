{
	"metadata": [
		{
			"src": [
				{
					"files": [ "**.cs" ],
					"src": "..",
					"exclude": [
						"**/obj/**",
						"**/bin/**",
						"**Tests**"
					]
				}
			],
			"dest": "api",
			"properties": {
				"TargetFramework": "net40"
			}
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml"
				]
			},
			{
				"files": [
					"api/*.md",
					"articles/**.md",
					"toc.yml",
					"*.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				],
				"exclude": [
					"obj/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"api/**.md"
				],
				"exclude": [
					"obj/**"
				]
			}
		],
		"dest": "../../docs",
		"globalMetadataFiles": [],
		"globalMetadata": {
			"_appTitle": "Yort.AfterPay.InStore",
			"_appFooter": "Copyright (c) 2017 Troy Willmot"
		},
		"fileMetadataFiles": [],
		"template": [ "statictoc", "memberpage/2.24.0/content" ],
		"postProcessors": [],
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false
	}
}